@model IEnumerable<EntityLayer.Concreate.Announcement>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

@if (Model.Count() > 0)
{
    <table class="table table-bordered">
        <tr>
            <th>Id</th>
            <th>Başlık</th>
            <th>İçerik</th>
        </tr>
        @foreach (var x in Model)
        {
            <tr>
                <td>@x.AnnouncementId</td>
                <td>
                    @x.AnnouncementTitle
                </td>
                <td>@x.AnnouncementSubject</td>
                
            </tr>
        }
    </table>
}
else
{
    <h2>Gösterilcek bir duyuru yok</h2>
}



