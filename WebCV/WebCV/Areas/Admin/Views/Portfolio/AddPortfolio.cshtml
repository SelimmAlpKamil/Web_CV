@model EntityLayer.Concreate.Portfolio

@{
    ViewData["Title"] = "AddPortfolio";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<form method="post" class="form-group" style="margin:10px">
    <label>Başlık</label>
    <input type="text" class="form-control" asp-for="PortfolioTitle" />
    <span asp-validation-for="PortfolioTitle" class="alert-danger"></span>
    <br />
    <label>Resim Adresi</label>
    <input type="text" class="form-control" asp-for="PortfolioImageURL" />
    <span asp-validation-for="PortfolioImageURL" class="alert-danger"></span>
    <br />
    <label>Link</label>
    <input type="text" class="form-control" asp-for="PortfolioURL" />
    <span asp-validation-for="PortfolioURL" class="alert-danger"></span>
    <br />
    <button class="btn btn-outline-primary">Ekle</button>

</form>


