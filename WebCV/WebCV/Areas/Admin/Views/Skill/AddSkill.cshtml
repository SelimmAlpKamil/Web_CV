@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@model EntityLayer.Concreate.Skill

@{
    ViewData["Title"] = "AddSkill";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post" class="form-group" style="margin:10px">
    <label>İsim</label>
    <input type="text" class="form-control" asp-for="SkillName" />
    <span class="alert-danger" asp-validation-for="SkillName"></span>
    <br />
    <label>Değer</label>
    <input type="text" class="form-control" asp-for="SkillValue" />
    <span class="alert-danger" asp-validation-for="SkillValue"></span>
    <br />
    <button class="btn btn-outline-info">Ekle</button>
</form>

