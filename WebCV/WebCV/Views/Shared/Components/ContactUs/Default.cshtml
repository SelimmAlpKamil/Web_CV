@model IEnumerable<EntityLayer.Concreate.About>

@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<div class="col-md-6" data-aos="fade-left" data-aos-delay="300">
    <div class="mt-3 px-1">
        <div class="h5">Benimle iletişime geçin</div>
        <p>Yanda yer alan form üzerinden benimle iletiime geçebilirsiniz ve ayrıca aşağıda yer alan bilgilerime ulaşabilirsiniz. </p>
        
    </div>
    <div class="mt-53 px-1">
        <div class="row">
            @foreach(var x in Model)
            {
                <div class="col-sm-2">
                    <div class="pb-1">mail:</div>
                </div>
                <div class="col-sm-10">
                    <div class="pb-1 fw-bolder">@x.AboutMail</div>
                </div>
                <div class="col-sm-2">
                    <div class="pb-1">Telefon:</div>
                </div>
                <div class="col-sm-10">
                    <div class="pb-1 fw-bolder">@x.AboutPhone</div>
                </div>
            }
        </div>
    </div>
</div>