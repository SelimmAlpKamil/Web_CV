
@{
    Layout = null;
}

<!DOCTYPE html>
<html>

@await Html.PartialAsync("/Views/UserLayoutPatial/_UserHeadPartial.cshtml")

<body>

    @await Html.PartialAsync("/Views/UserLayoutPatial/_UserSideBarPartial.cshtml")



    <div class="main-content" id="panel">

        @await Html.PartialAsync("/Views/UserLayoutPatial/_UserNavbarPartial.cshtml")

        <div class="header bg-primary pb-6">
            <div class="container-fluid">
                <div class="header-body">
                </div>
            </div>
        </div>


        <div class="container-fluid mt--6">
            <div class="row">
                <div class="col">
                    <div class="card border-0">
                        @RenderBody()
                    </div>
                </div>
            </div>

            @await Html.PartialAsync("/Views/UserLayoutPatial/_UserFooterPartial.cshtml")
            

        </div>
    </div>

    @await Html.PartialAsync("/Views/UserLayoutPatial/_UserScriptPartial.cshtml")

</body>

</html>
